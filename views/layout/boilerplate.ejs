    <style>
            /* GRID */
.row {
  max-width: 1140px; /* 114rem * 10 = 1140px */
  margin: 0 auto;
}

.row:not(:last-child) {
  margin-bottom: 80px; /* 8rem * 10 = 80px */
}

@media (max-width: 562.5px) { /* 56.25em * 10 = 562.5px */
  .row:not(:last-child) {
    margin-bottom: 60px; /* 6rem * 10 = 60px */
  }
}

@media (max-width: 562.5px) {
  .row {
    max-width: 500px; /* 50rem * 10 = 500px */
    padding: 0 30px; /* 3rem * 10 = 30px */
  }
}

.row::after {
  content: "";
  display: table;
  clear: both;
}

.row [class^=col-] {
  float: left;
}

.row [class^=col-]:not(:last-child) {
  margin-right: 60px; /* 6rem * 10 = 60px */
}

@media (max-width: 562.5px) {
  .row [class^=col-]:not(:last-child) {
    margin-right: 0;
    margin-bottom: 60px; /* 6rem * 10 = 60px */
  }
}

@media (max-width: 562.5px) {
  .row [class^=col-] {
    width: 100% !important;
  }
}

.row .col-1-of-2 {
  width: calc((100% - 60px) / 2); /* 6rem * 10 = 60px */
}

.row .col-1-of-3 {
  width: calc((100% - 2 * 60px) / 3); /* 2 * 6rem * 10 = 120px */
}

.row .col-2-of-3 {
  width: calc(2 * (100% - 2 * 60px) / 3 + 60px); /* 2 * 6rem * 10 = 120px */
}

.row .col-1-of-4 {
  width: calc((100% - 3 * 60px) / 4); /* 3 * 6rem * 10 = 180px */
}

.row .col-2-of-4 {
  width: calc(2 * (100% - 3 * 60px) / 4 + 60px); /* 3 * 6rem * 10 = 180px */
}

.row .col-3-of-4 {
  width: calc(3 * (100% - 3 * 60px) / 4 + 2 * 60px); /* 3 * 6rem * 10 = 180px */
}
    </style>
